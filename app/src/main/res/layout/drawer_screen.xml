<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:weightSum="10"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout

            android:weightSum="10"
            android:gravity="left"
            android:orientation="vertical"
            android:layout_weight="7"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_weight="2"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

<!--                <com.airbnb.lottie.LottieAnimationView-->
<!--                    android:id="@+id/animation"-->
<!--                    app:lottie_loop="true"-->
<!--                    android:scaleX="1.3"-->
<!--                    android:scaleY="1.9"-->
<!--                    app:lottie_autoPlay="true"-->
<!--                   android:layout_width="match_parent"-->
<!--                    android:layout_height="170dp">-->

<!--                </com.airbnb.lottie.LottieAnimationView>-->
                <ImageView
                    android:layout_gravity="center"
                    android:padding="10dp"
                    android:src="@drawable/vaulty"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

                </LinearLayout>

            <LinearLayout
                android:layout_weight="8"
                android:orientation="vertical"
                android:gravity="bottom"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:paddingLeft="10dp"
                    android:textSize="23dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:text="Vaulty"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>


            </LinearLayout>


        </LinearLayout>


        <LinearLayout
            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="1dp"></LinearLayout>

        <LinearLayout
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:orientation="vertical"
            android:layout_weight="2.9"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:id="@+id/privatepolicy"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/black"
                    android:text="Private policy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/history"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="History"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/changepin"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Change Pin"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/changequestion"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Change Security Question"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/Fingerprint"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:layout_marginTop="2dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Fingerprint"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/setting"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:layout_marginTop="2dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Setting"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/rate"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Rate us"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/premmium"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Premium"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/term"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Term &amp; condition"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/help"
                    android:textSize="18dp"
                    android:padding="10dp"
                    android:paddingLeft="20dp"
                    android:fontFamily="@font/hindsiliguriregular"
                    android:textColor="@color/black"
                    android:text="Help"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:background="@color/black"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"></LinearLayout>

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</LinearLayout>